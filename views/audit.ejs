<!doctype html>
<html>
  <head>
    <title>Welcome to <%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopData.shopName%></h1>
    <h2>Here are all login attempts</h2>
    <ul>
        <% attempts.forEach(attempt => { %>
            <li> <%=attempt.outcome%>: <%=attempt.username%>@<%=attempt.time%> </li> 
        <% }) %>
    </ul>
  </body>
</html>
